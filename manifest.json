{
    "manifest_version": 2,
    "name": "Name of extension",
    "version": "1.0",
    "description": "Description",
    "permissions": ["<all_urls>"],
    "page_action": {
        "default_title": "Default title"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "browser_specific_settings": {
        "gecko": {
            "id": "tamago@egg.com",
            "strict_min_version": "48.0"
        }
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["contentScript.js"]
        }
    ],
    "browser_action": {}
}
