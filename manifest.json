{
    "manifest_version": 2,
    "name": "Name of extension",
    "version": "1.0",
    "description": "Description",
    "permissions": ["<all_urls>"],
    "background": {
        "scripts": ["background.js"]
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["contentScript.js"]
        }
    ],
    "browser_action": {
        "default_title": "Default title",
        "default_icon": "hello_extensions.png"
    }
}
